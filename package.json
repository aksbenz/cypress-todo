{
  "name": "cypress-todo",
  "version": "1.0.0",
  "description": "Cypress Sample Todo Tests",
  "main": "index.js",
  "scripts": {
    "test": "npx cypress open",
    "test:chrome": "cypress run --browser chrome",
    "report:merge": "npx mochawesome-merge cypress/results/json/*.json > index.json",
    "report:generate": "npx marge index.json --reportDir public --assetsDir public/assets --reportPageTitle index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aksbenz/cypress-todo.git"
  },
  "keywords": [
    "cypress",
    "test",
    "automation",
    "todo"
  ],
  "author": "aksbenz",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/aksbenz/cypress-todo/issues"
  },
  "homepage": "https://github.com/aksbenz/cypress-todo#readme",
  "devDependencies": {
    "cypress": "^4.9.0",
    "cypress-multi-reporters": "^1.4.0",
    "mocha": "^8.0.1",
    "mochawesome": "^6.1.1",
    "mochawesome-merge": "^4.1.0",
    "mochawesome-report-generator": "^5.1.0"
  }
}
